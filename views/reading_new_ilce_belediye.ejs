<div class="row edit-header">
 <div class="col-xs-12">
 		<h3><%= title %></h3>
  	Resimdeki sayıları resmin altındaki forma yazınız.
	</div>
</div>

<div class="row">
	<div class="col-xs-12">
		<p>
			<h4><%= evidence.city || "(Şehir)" %> &rarr; <%= evidence.district || "(İlçe)" %> &rarr; Sandık <%= evidence.no || "(No)" %></h4>
			<%= types.evidence[evidence.type] || "(Tutanak tipi)" %>
		</p>
  </div>
</div>

<div class="row ">
  <div class="col-xs-12">
    <img src="<%=s3path + evidence.img %>" width="100%" class="tutanak">
  </div>
</div>

